{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Tech-Challenge (module)",
      "type": "python",
      "request": "launch",
      "module": "src.main",
      "cwd": "${workspaceFolder:tech-challenge}",
      "justMyCode": false
    }
  ]
}